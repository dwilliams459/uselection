<path fill="@FillColor" stroke="#ffffff" @onclick="OnStateClick" class="@State.Class" opacity="1" stroke-opacity="1" stroke-width="1.875" stroke-linejoin="round" transform="@State.Transform" style="@State.Style"fill-opacity="1" d="@State.Path" /> 
