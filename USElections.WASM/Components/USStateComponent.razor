{
    <rect x="@State.LabelX" y="@State.LabelY" fill="@FillColor" width="45" height="28.749999999999996" r="5.749999999999999" rx="5.749999999999999" ry="5.749999999999999" stroke="#ffffff" transform="matrix(1,0,0,1,0,0)" opacity="1" stroke-opacity="1" stroke-width="1.875" stroke-linejoin="round" style="-webkit-tap-highlight-color: rgba(0, 0, 0, 0); opacity: 1; cursor: pointer; stroke-opacity: 1; stroke-linejoin: round; fill-opacity: 1;" fill-opacity="1"/>
    @BulletText()
    <rect x="@State.LabelX" y="@State.LabelY" @onclick="OnStateClick" width="45" height="28.749999999999996" r="5.749999999999999" rx="5.749999999999999" ry="5.749999999999999" stroke="#ffffff" transform="matrix(1,0,0,1,0,0)" opacity="0" stroke-opacity="0" stroke-width="1.875" stroke-linejoin="round" style="-webkit-tap-highlight-color: rgba(0, 0, 0, 0); opacity: 0; cursor: pointer; stroke-opacity: 0; stroke-linejoin: round; fill-opacity: 0;" fill-opacity="0"/>

}    
